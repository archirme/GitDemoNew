<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="SeleniumTesting.E2EProjectNew.Listeners" />
	</listeners>

	<test name="Validating User login">
		<classes>
			<class name="SeleniumTesting.E2EProjectNew.HomePage" />
		</classes>
	</test>

	<test name="Validating title in the Page">
		<classes>
			<class name="SeleniumTesting.E2EProjectNew.validateTitle" />
		</classes>
	</test>
	<test name="App Testing">
		<classes>
			<class name="SeleniumTesting.E2EProjectNew.AppTest" />
		</classes>
	</test>
	<test name="Validating navigation Bar">
		<classes>
			<class
				name="SeleniumTesting.E2EProjectNew.validateNavigationBar" />
		</classes>
	</test>

</suite> <!-- Suite -->
